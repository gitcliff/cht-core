<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>RDT Covid-19 - Capture</h:title>
    <model>
      <itext>
        <translation lang="en">
          <text id="/covid19-rdt-capture/capture/note_continue_1:label">
            <value>âœ“ Results captured!</value>
          </text>
          <text id="/covid19-rdt-capture/capture/note_continue_2:label">
            <value>Click "Next" to continue.</value>
          </text>
          <text id="/covid19-rdt-capture/capture/rdtoolkit-capture/note_instructions_provision:label">
            <value>Click on "Launch app" to capture the result of the Covid-19 rapid diagnostic test for this patient.</value>
          </text>
          <text id="/covid19-rdt-capture/capture:label">
            <value>Launch</value>
          </text>
          <text id="/covid19-rdt-capture/inputs/contact/_id:label">
            <value>What is the patient's name?</value>
          </text>
          <text id="/covid19-rdt-capture/repeat-test/repeat_test/no:label">
            <value>No</value>
          </text>
          <text id="/covid19-rdt-capture/repeat-test/repeat_test/yes:label">
            <value>Yes</value>
          </text>
          <text id="/covid19-rdt-capture/repeat-test/repeat_test:label">
            <value>Does the patient require to repeat the test?</value>
          </text>
          <text id="/covid19-rdt-capture/repeat-test/repeat_test_reason/indeterminate:label">
            <value>Indeterminate</value>
          </text>
          <text id="/covid19-rdt-capture/repeat-test/repeat_test_reason/invalid:label">
            <value>Invalid</value>
          </text>
          <text id="/covid19-rdt-capture/repeat-test/repeat_test_reason/lost:label">
            <value>Lost</value>
          </text>
          <text id="/covid19-rdt-capture/repeat-test/repeat_test_reason/other:label">
            <value>Other</value>
          </text>
          <text id="/covid19-rdt-capture/repeat-test/repeat_test_reason:label">
            <value>What is the reason?</value>
          </text>
          <text id="/covid19-rdt-capture/repeat-test/repeat_test_reason_other:label">
            <value>Specify other</value>
          </text>
          <text id="/covid19-rdt-capture/repeat-test:label">
            <value>Test Repeat</value>
          </text>
          <text id="/covid19-rdt-capture/summary/summary_details:label">
            <value>Patient Details&lt;i class="fa fa-user"&gt;&lt;/i&gt;</value>
          </text>
          <text id="/covid19-rdt-capture/summary/summary_header:label">
            <value>Be sure you submit to complete this action</value>
          </text>
          <text id="/covid19-rdt-capture/summary/summary_info:label">
            <value>RDT Details&lt;i class="fa fa-medkit"&gt;&lt;/i&gt;</value>
          </text>
          <text id="/covid19-rdt-capture/summary/summary_name:label">
            <value>**<output value=" /covid19-rdt-capture/patient_name "/>**
<output value=" /covid19-rdt-capture/inputs/contact/sex "/>
<output value=" /covid19-rdt-capture/age "/></value></text>
          <text id="/covid19-rdt-capture/summary/summary_rdt_loinc_long_common_name:label">
            <value>SARS-CoV-2 (COVID-19) Ag [Presence] in Respiratory specimen by Rapid immunoassay</value>
          </text>
          <text id="/covid19-rdt-capture/summary/summary_results:label">
            <value>**Results:** <output value=" /covid19-rdt-capture/summary/result "/></value>
          </text>
          <text id="/covid19-rdt-capture/summary/summary_results_expired:label">
            <value>Test expired&lt;i class="fa fa-exclamation-triangle"&gt;&lt;/i&gt;</value>
          </text>
          <text id="/covid19-rdt-capture/summary/summary_results_expired_2:label">
            <value>The rapid diagnostic test has expired. Please be sure it is allowed to continue.</value>
          </text>
        </translation>
      </itext>
      <instance>
        <covid19-rdt-capture delimiter="#" id="covid19_rdt_capture" prefix="J1!covid19_rdt_capture!" version="2021-09-03_05-35">
          <inputs tag="hidden">
            <source>user</source>
            <contact>
              <_id/>
              <patient_id/>
              <first_name/>
              <last_name/>
              <name/>
              <date_of_birth/>
              <sex/>
              <phone/>
              <phone_alternate/>
              <address/>
              <parent>
                <_id/>
                <name/>
                <address/>
                <parent>
                  <_id/>
                  <name/>
                  <address/>
                </parent>
              </parent>
            </contact>
          </inputs>
          <patient_name/>
          <patient_age_in_years/>
          <patient_age_in_months/>
          <patient_age_in_days/>
          <age/>
          <test-information tag="hidden">
            <session_id/>
            <administrator_id/>
            <administrator_name/>
            <test_id/>
            <facility_id/>
            <facility_name/>
            <facility_address/>
            <facility_test_setting/>
            <other_test_setting/>
            <test_reason/>
            <symptoms/>
            <days_since_symptoms_began/>
            <specimen_type/>
            <specimen_type_other/>
            <rdt_lot/>
            <rdt_lot_expiry_date/>
            <additional_notes/>
          </test-information>
          <capture>
            <rdtoolkit-capture>
              <action>org.rdtoolkit.action.Capture</action>
              <note_instructions_provision/>
              <android-app-inputs>
                <rdt_session_id/>
              </android-app-inputs>
              <android-app-outputs>
                <rdt_session_bundle>
                  <rdt_session_state/>
                  <rdt_session_time_started/>
                  <rdt_session_time_resolved/>
                  <rdt_session_time_expired/>
                  <rdt_session_test_profile/>
                  <rdt_session_result_bundle>
                    <rdt_session_result_time_read/>
                    <rdt_session_result_extra_images>
                      <cropped/>
                    </rdt_session_result_extra_images>
                    <rdt_session_result_map>
                      <sars_cov2/>
                    </rdt_session_result_map>
                  </rdt_session_result_bundle>
                </rdt_session_bundle>
              </android-app-outputs>
            </rdtoolkit-capture>
            <note_continue_1/>
            <note_continue_2/>
          </capture>
          <repeat-test>
            <repeat_test/>
            <repeat_test_reason/>
            <repeat_test_reason_other/>
          </repeat-test>
          <summary>
            <time_started tag="hidden"/>
            <time_read tag="hidden"/>
            <time_expired tag="hidden"/>
            <results_expired tag="hidden"/>
            <result tag="hidden"/>
            <summary_header/>
            <summary_details/>
            <summary_name/>
            <summary_info/>
            <summary_rdt_loinc_long_common_name/>
            <summary_results_expired/>
            <summary_results_expired_2/>
            <summary_results/>
            <summary_image type="binary"/>
          </summary>
          <patient_uuid tag="hidden"/>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </covid19-rdt-capture>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/covid19-rdt-capture/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/covid19-rdt-capture/inputs/source" type="string"/>
      <bind nodeset="/covid19-rdt-capture/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/covid19-rdt-capture/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/covid19-rdt-capture/inputs/contact/first_name" type="string"/>
      <bind nodeset="/covid19-rdt-capture/inputs/contact/last_name" type="string"/>
      <bind nodeset="/covid19-rdt-capture/inputs/contact/name" type="string"/>
      <bind nodeset="/covid19-rdt-capture/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/covid19-rdt-capture/inputs/contact/sex" type="string"/>
      <bind nodeset="/covid19-rdt-capture/inputs/contact/phone" type="string"/>
      <bind nodeset="/covid19-rdt-capture/inputs/contact/phone_alternate" type="string"/>
      <bind nodeset="/covid19-rdt-capture/inputs/contact/address" type="string"/>
      <bind nodeset="/covid19-rdt-capture/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/covid19-rdt-capture/inputs/contact/parent/name" type="string"/>
      <bind nodeset="/covid19-rdt-capture/inputs/contact/parent/address" type="string"/>
      <bind nodeset="/covid19-rdt-capture/inputs/contact/parent/parent/_id" type="string"/>
      <bind nodeset="/covid19-rdt-capture/inputs/contact/parent/parent/name" type="string"/>
      <bind nodeset="/covid19-rdt-capture/inputs/contact/parent/parent/address" type="string"/>
      <bind calculate="../inputs/contact/name" nodeset="/covid19-rdt-capture/patient_name" type="string"/>
      <bind calculate="floor( difference-in-months(  /covid19-rdt-capture/inputs/contact/date_of_birth , today() ) div 12 )" nodeset="/covid19-rdt-capture/patient_age_in_years" type="string"/>
      <bind calculate="difference-in-months(  /covid19-rdt-capture/inputs/contact/date_of_birth , today() )" nodeset="/covid19-rdt-capture/patient_age_in_months" type="string"/>
      <bind calculate="floor(decimal-date-time(today()) - decimal-date-time( /covid19-rdt-capture/inputs/contact/date_of_birth ) )" nodeset="/covid19-rdt-capture/patient_age_in_days" type="string"/>
      <bind calculate="if( /covid19-rdt-capture/patient_age_in_days  &lt; 31, 
concat( /covid19-rdt-capture/patient_age_in_days , ' days old'),
if( /covid19-rdt-capture/patient_age_in_months  &lt; 12, 
concat( /covid19-rdt-capture/patient_age_in_months , ' months old'), 
concat( /covid19-rdt-capture/patient_age_in_years , ' years old')))" nodeset="/covid19-rdt-capture/age" type="string"/>
      <bind nodeset="/covid19-rdt-capture/test-information/session_id" type="string"/>
      <bind nodeset="/covid19-rdt-capture/test-information/administrator_id" type="string"/>
      <bind nodeset="/covid19-rdt-capture/test-information/administrator_name" type="string"/>
      <bind nodeset="/covid19-rdt-capture/test-information/test_id" type="string"/>
      <bind nodeset="/covid19-rdt-capture/test-information/facility_id" type="string"/>
      <bind nodeset="/covid19-rdt-capture/test-information/facility_name" type="string"/>
      <bind nodeset="/covid19-rdt-capture/test-information/facility_address" type="string"/>
      <bind nodeset="/covid19-rdt-capture/test-information/facility_test_setting" type="string"/>
      <bind nodeset="/covid19-rdt-capture/test-information/other_test_setting" type="string"/>
      <bind nodeset="/covid19-rdt-capture/test-information/test_reason" type="string"/>
      <bind nodeset="/covid19-rdt-capture/test-information/symptoms" type="string"/>
      <bind nodeset="/covid19-rdt-capture/test-information/days_since_symptoms_began" type="string"/>
      <bind nodeset="/covid19-rdt-capture/test-information/specimen_type" type="string"/>
      <bind nodeset="/covid19-rdt-capture/test-information/specimen_type_other" type="string"/>
      <bind nodeset="/covid19-rdt-capture/test-information/rdt_lot" type="string"/>
      <bind nodeset="/covid19-rdt-capture/test-information/rdt_lot_expiry_date" type="string"/>
      <bind nodeset="/covid19-rdt-capture/test-information/additional_notes" type="string"/>
      <bind nodeset="/covid19-rdt-capture/capture/rdtoolkit-capture/action" type="string"/>
      <bind nodeset="/covid19-rdt-capture/capture/rdtoolkit-capture/note_instructions_provision" readonly="true()" type="string"/>
      <bind calculate="../../../../test-information/test_id" nodeset="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-inputs/rdt_session_id" type="string"/>
      <bind nodeset="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_state" required="true()" type="string"/>
      <bind nodeset="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_time_started" type="string"/>
      <bind nodeset="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_time_resolved" type="string"/>
      <bind nodeset="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_time_expired" type="string"/>
      <bind nodeset="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_test_profile" type="string"/>
      <bind nodeset="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_time_read" type="string"/>
      <bind nodeset="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_extra_images/cropped" type="string"/>
      <bind nodeset="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_map/sars_cov2" required="true()" type="string"/>
      <bind nodeset="/covid19-rdt-capture/capture/note_continue_1" readonly="true()" relevant=" /covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_time_read  != ''" type="string"/>
      <bind nodeset="/covid19-rdt-capture/capture/note_continue_2" readonly="true()" relevant=" /covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_time_read  != ''" type="string"/>
      <bind nodeset="/covid19-rdt-capture/repeat-test/repeat_test" required="true()" type="select1"/>
      <bind nodeset="/covid19-rdt-capture/repeat-test/repeat_test_reason" relevant="selected(../repeat_test , 'yes')" type="select1"/>
      <bind nodeset="/covid19-rdt-capture/repeat-test/repeat_test_reason_other" relevant="selected(../repeat_test_reason , 'other')" type="string"/>
      <bind nodeset="/covid19-rdt-capture/summary" relevant=" /covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_state  != ''"/>
      <bind calculate="format-date-time(parse-timestamp-to-date( /covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_time_started ), &quot;%e %b, %Y at %H:%M:%S&quot;)" nodeset="/covid19-rdt-capture/summary/time_started" type="string"/>
      <bind calculate="format-date-time(parse-timestamp-to-date( /covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_time_read ), &quot;%e %b, %Y at %H:%M:%S&quot;)" nodeset="/covid19-rdt-capture/summary/time_read" type="string"/>
      <bind calculate="format-date-time(parse-timestamp-to-date( /covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_time_expired ), &quot;%e %b, %Y at %H:%M:%S&quot;)" nodeset="/covid19-rdt-capture/summary/time_expired" type="string"/>
      <bind calculate="if( /covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_time_read  &gt;  /covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_time_expired , 'TRUE', 'FALSE')" nodeset="/covid19-rdt-capture/summary/results_expired" type="string"/>
      <bind calculate="if( /covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_map/sars_cov2 ='sars_cov2_pos', 'Positive', if( /covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_map/sars_cov2 ='sars_cov2_neg', 'Negative', 'Invalid result'))" nodeset="/covid19-rdt-capture/summary/result" type="string"/>
      <bind nodeset="/covid19-rdt-capture/summary/summary_header" readonly="true()" type="string"/>
      <bind nodeset="/covid19-rdt-capture/summary/summary_details" readonly="true()" type="string"/>
      <bind nodeset="/covid19-rdt-capture/summary/summary_name" readonly="true()" type="string"/>
      <bind nodeset="/covid19-rdt-capture/summary/summary_info" readonly="true()" type="string"/>
      <bind nodeset="/covid19-rdt-capture/summary/summary_rdt_loinc_long_common_name" readonly="true()" type="string"/>
      <bind nodeset="/covid19-rdt-capture/summary/summary_results_expired" readonly="true()" relevant=" /covid19-rdt-capture/summary/results_expired  ='FALSE'" type="string"/>
      <bind nodeset="/covid19-rdt-capture/summary/summary_results_expired_2" readonly="true()" relevant=" /covid19-rdt-capture/summary/results_expired  ='FALSE'" type="string"/>
      <bind nodeset="/covid19-rdt-capture/summary/summary_results" readonly="true()" required="true()" type="string"/>
      <bind calculate=" /covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_extra_images/cropped " nodeset="/covid19-rdt-capture/summary/summary_image" type="string"/>
      <bind calculate="../inputs/contact/_id" nodeset="/covid19-rdt-capture/patient_uuid" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/covid19-rdt-capture/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group ref="/covid19-rdt-capture/inputs">
      <group appearance="field-list" ref="/covid19-rdt-capture/inputs/contact">
        <input appearance="db-object" ref="/covid19-rdt-capture/inputs/contact/_id">
          <label ref="jr:itext('/covid19-rdt-capture/inputs/contact/_id:label')"/>
        </input>
        <group ref="/covid19-rdt-capture/inputs/contact/parent">
          <group ref="/covid19-rdt-capture/inputs/contact/parent/parent"/>
        </group>
      </group>
    </group>
    <group ref="/covid19-rdt-capture/test-information"/>
    <group appearance="field-list" ref="/covid19-rdt-capture/capture">
      <label ref="jr:itext('/covid19-rdt-capture/capture:label')"/>
      <group appearance="android-app-launcher" ref="/covid19-rdt-capture/capture/rdtoolkit-capture">
        <input appearance="hidden" ref="/covid19-rdt-capture/capture/rdtoolkit-capture/action"/>
        <input ref="/covid19-rdt-capture/capture/rdtoolkit-capture/note_instructions_provision">
          <label ref="jr:itext('/covid19-rdt-capture/capture/rdtoolkit-capture/note_instructions_provision:label')"/>
        </input>
        <group appearance="android-app-inputs" ref="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-inputs">
          <input appearance="hidden" ref="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-inputs/rdt_session_id"/>
        </group>
        <group appearance="android-app-outputs" ref="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs">
          <group appearance="android-app-object" ref="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle">
            <input appearance="hidden" ref="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_state"/>
            <input appearance="hidden" ref="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_time_started"/>
            <input appearance="hidden" ref="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_time_resolved"/>
            <input appearance="hidden" ref="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_time_expired"/>
            <input appearance="hidden" ref="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_test_profile"/>
            <group appearance="android-app-object" ref="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle">
              <input appearance="hidden" ref="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_time_read"/>
              <group appearance="android-app-object" ref="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_extra_images">
                <input appearance="hidden" ref="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_extra_images/cropped"/>
              </group>
              <group appearance="android-app-object" ref="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_map">
                <input appearance="hidden" ref="/covid19-rdt-capture/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_map/sars_cov2"/>
              </group>
            </group>
          </group>
        </group>
      </group>
      <input ref="/covid19-rdt-capture/capture/note_continue_1">
        <label ref="jr:itext('/covid19-rdt-capture/capture/note_continue_1:label')"/>
      </input>
      <input ref="/covid19-rdt-capture/capture/note_continue_2">
        <label ref="jr:itext('/covid19-rdt-capture/capture/note_continue_2:label')"/>
      </input>
    </group>
    <group appearance="field-list" ref="/covid19-rdt-capture/repeat-test">
      <label ref="jr:itext('/covid19-rdt-capture/repeat-test:label')"/>
      <select1 ref="/covid19-rdt-capture/repeat-test/repeat_test">
        <label ref="jr:itext('/covid19-rdt-capture/repeat-test/repeat_test:label')"/>
        <item>
          <label ref="jr:itext('/covid19-rdt-capture/repeat-test/repeat_test/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/covid19-rdt-capture/repeat-test/repeat_test/no:label')"/>
          <value>no</value>
        </item>
      </select1>
      <select1 ref="/covid19-rdt-capture/repeat-test/repeat_test_reason">
        <label ref="jr:itext('/covid19-rdt-capture/repeat-test/repeat_test_reason:label')"/>
        <item>
          <label ref="jr:itext('/covid19-rdt-capture/repeat-test/repeat_test_reason/invalid:label')"/>
          <value>invalid</value>
        </item>
        <item>
          <label ref="jr:itext('/covid19-rdt-capture/repeat-test/repeat_test_reason/indeterminate:label')"/>
          <value>indeterminate</value>
        </item>
        <item>
          <label ref="jr:itext('/covid19-rdt-capture/repeat-test/repeat_test_reason/lost:label')"/>
          <value>lost</value>
        </item>
        <item>
          <label ref="jr:itext('/covid19-rdt-capture/repeat-test/repeat_test_reason/other:label')"/>
          <value>other</value>
        </item>
      </select1>
      <input ref="/covid19-rdt-capture/repeat-test/repeat_test_reason_other">
        <label ref="jr:itext('/covid19-rdt-capture/repeat-test/repeat_test_reason_other:label')"/>
      </input>
    </group>
    <group appearance="field-list summary" ref="/covid19-rdt-capture/summary">
      <input appearance="h2" ref="/covid19-rdt-capture/summary/summary_header">
        <label ref="jr:itext('/covid19-rdt-capture/summary/summary_header:label')"/>
      </input>
      <input appearance="h1 yellow" ref="/covid19-rdt-capture/summary/summary_details">
        <label ref="jr:itext('/covid19-rdt-capture/summary/summary_details:label')"/>
      </input>
      <input appearance="center" ref="/covid19-rdt-capture/summary/summary_name">
        <label ref="jr:itext('/covid19-rdt-capture/summary/summary_name:label')"/>
