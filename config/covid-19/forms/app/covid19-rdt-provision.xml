      <instance>
        <covid19-rdt-provision delimiter="#" id="covid19:rdt:provision" prefix="J1!covid19:rdt:provision!" version="2021-09-02_02-00">
          <inputs tag="hidden">
            <user>
              <contact_id/>
              <facility_id/>
              <name/>
            </user>
            <source>user</source>
            <contact>
              <_id/>
              <patient_id/>
              <first_name/>
              <last_name/>
              <name/>
              <date_of_birth/>
              <sex/>
              <phone/>
              <phone_alternate/>
              <address/>
              <age/>
              <parent>
                <_id/>
                <name/>
                <address/>
                <parent>
                  <_id/>
                  <name/>
                  <address/>
                </parent>
              </parent>
            </contact>
          </inputs>
          <test-reference>
            <case_id/>
            <test_id/>
            <facility_id/>
            <facility_name/>
            <facility_address/>
            <facility_confirm_ask>yes</facility_confirm_ask>
            <other_facility_name/>
            <other_facility_address/>
            <other_facility_test_setting/>
            <other_test_setting/>
            <gps_ask tag="hidden"/>
            <no_gps_text tag="hidden"/>
            <gps/>
            <test_reason/>
            <symptoms/>
            <days_since_symptoms_began/>
            <specimen_type/>
            <specimen_type_other/>
            <rdt_lot/>
            <rdt_lot_expiry_date/>
            <additional_notes/>
          </test-reference>
          <provision>
            <rdtoolkit-provision tag="hidden">
              <action>org.rdtoolkit.action.Provision</action>
              <note_instructions_provision/>
              <android-app-inputs>
                <rdt_session_id/>
                <rdt_config_bundle>
                  <rdt_config_classifier_mode>PRE_POPULATE</rdt_config_classifier_mode>
                  <rdt_config_session_type>TWO_PHASE</rdt_config_session_type>
                  <rdt_config_provision_mode>CRITERIA_SET_AND</rdt_config_provision_mode>
                  <rdt_config_provision_mode_data>sars_cov2</rdt_config_provision_mode_data>
                  <rdt_config_flavor_one/>
                  <rdt_config_flavor_two/>
                  <rdt_config_cloudworks_dns/>
                </rdt_config_bundle>
              </android-app-inputs>
              <android-app-outputs>
                <rdt_session_bundle>
                  <rdt_session_state/>
                  <rdt_session_time_started/>
                  <rdt_session_time_resolved/>
                  <rdt_session_time_expired/>
                  <rdt_session_test_profile/>
                </rdt_session_bundle>
              </android-app-outputs>
            </rdtoolkit-provision>
            <preview-output>
              <preview_test_profile/>
              <preview_status/>
              <preview_time_started/>
              <preview_time_resolved/>
              <preview_time_expired/>
              <syndrome_test_type>COVID-19</syndrome_test_type>
              <rdt_loinc_code>94558-4</rdt_loinc_code>
              <rdt_loinc_long_common_name>SARS-CoV-2 (COVID-19) Ag [Presence] in Respiratory specimen by Rapid immunoassay</rdt_loinc_long_common_name>
              <note_instructions_registration tag="hidden"/>
            </preview-output>
          </provision>
          <patient_uuid tag="hidden"/>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </covid19-rdt-provision>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/covid19-rdt-provision/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/covid19-rdt-provision/inputs/user/contact_id" type="string"/>
      <bind nodeset="/covid19-rdt-provision/inputs/user/facility_id" type="string"/>
      <bind nodeset="/covid19-rdt-provision/inputs/user/name" type="string"/>
      <bind nodeset="/covid19-rdt-provision/inputs/source" type="string"/>
