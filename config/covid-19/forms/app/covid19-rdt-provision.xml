      <bind nodeset="/covid19-rdt-provision/inputs/contact/phone_alternate" type="string"/>
      <bind nodeset="/covid19-rdt-provision/inputs/contact/address" type="string"/>
      <bind calculate="floor( difference-in-months( ../date_of_birth, today() ) div 12 )" nodeset="/covid19-rdt-provision/inputs/contact/age" type="string"/>
      <bind nodeset="/covid19-rdt-provision/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/covid19-rdt-provision/inputs/contact/parent/name" type="string"/>
      <bind nodeset="/covid19-rdt-provision/inputs/contact/parent/address" type="string"/>
      <bind nodeset="/covid19-rdt-provision/inputs/contact/parent/parent/_id" type="string"/>
      <bind nodeset="/covid19-rdt-provision/inputs/contact/parent/parent/name" type="string"/>
      <bind nodeset="/covid19-rdt-provision/inputs/contact/parent/parent/address" type="string"/>
      <bind calculate="coalesce(., uuid())" nodeset="/covid19-rdt-provision/test-reference/case_id" required="true()" type="string"/>
      <bind calculate="coalesce(., uuid())" nodeset="/covid19-rdt-provision/test-reference/test_id" required="true()" type="string"/>
      <bind calculate="if(../../inputs/contact/parent/parent/_id != '', ../../inputs/contact/parent/parent/_id, ../../inputs/contact/parent/_id)" nodeset="/covid19-rdt-provision/test-reference/facility_id" relevant="selected(../facility_confirm_ask, 'yes')" type="string"/>
      <bind calculate="if(../../inputs/contact/parent/parent/_id != '', ../../inputs/contact/parent/parent/name, ../../inputs/contact/parent/name)" nodeset="/covid19-rdt-provision/test-reference/facility_name" relevant="selected(../facility_confirm_ask, 'yes')" type="string"/>
      <bind calculate="if(../../inputs/contact/parent/parent/_id != '', ../../inputs/contact/parent/parent/address, ../../inputs/contact/parent/address)" nodeset="/covid19-rdt-provision/test-reference/facility_address" relevant="selected(../facility_confirm_ask, 'yes')" type="string"/>
      <bind nodeset="/covid19-rdt-provision/test-reference/facility_confirm_ask" type="select1"/>
      <bind nodeset="/covid19-rdt-provision/test-reference/other_facility_name" relevant="selected(../facility_confirm_ask, 'no')" type="string"/>
      <bind nodeset="/covid19-rdt-provision/test-reference/other_facility_address" relevant="selected(../facility_confirm_ask, 'no')" type="string"/>
      <bind nodeset="/covid19-rdt-provision/test-reference/other_facility_test_setting" relevant="selected(../facility_confirm_ask, 'no')" type="select1"/>
      <bind nodeset="/covid19-rdt-provision/test-reference/other_test_setting" relevant="(selected(../other_facility_test_setting , 'community_other') or selected(../other_facility_test_setting , 'healthcare_other'))" type="string"/>
      <bind nodeset="/covid19-rdt-provision/test-reference/gps_ask" type="geopoint"/>
      <bind nodeset="/covid19-rdt-provision/test-reference/no_gps_text" readonly="true()" relevant="../gps = ''" type="string"/>
      <bind calculate="if(../gps_ask = '', . , ../gps_ask)" nodeset="/covid19-rdt-provision/test-reference/gps" type="string"/>
      <bind nodeset="/covid19-rdt-provision/test-reference/test_reason" type="select"/>
      <bind nodeset="/covid19-rdt-provision/test-reference/symptoms" type="select"/>
      <bind constraint=". &amp;gt;= 1 and . &amp;lt;= 200" jr:constraintMsg="jr:itext('/covid19-rdt-provision/test-reference/days_since_symptoms_began:jr:constraintMsg')" nodeset="/covid19-rdt-provision/test-reference/days_since_symptoms_began" relevant="../symptoms != ''" type="int"/>
      <bind nodeset="/covid19-rdt-provision/test-reference/specimen_type" type="select1"/>
      <bind nodeset="/covid19-rdt-provision/test-reference/specimen_type_other" relevant="selected(../specimen_type, 'other')" type="string"/>
      <bind nodeset="/covid19-rdt-provision/test-reference/rdt_lot" type="string"/>
      <bind nodeset="/covid19-rdt-provision/test-reference/rdt_lot_expiry_date" type="date"/>
      <bind nodeset="/covid19-rdt-provision/test-reference/additional_notes" type="string"/>
      <bind nodeset="/covid19-rdt-provision/provision/rdtoolkit-provision/action" type="string"/>
      <bind nodeset="/covid19-rdt-provision/provision/rdtoolkit-provision/note_instructions_provision" readonly="true()" type="string"/>
      <bind calculate=" /covid19-rdt-provision/test-reference/test_id " nodeset="/covid19-rdt-provision/provision/rdtoolkit-provision/android-app-inputs/rdt_session_id" type="string"/>
      <bind nodeset="/covid19-rdt-provision/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_classifier_mode" type="string"/>
      <bind nodeset="/covid19-rdt-provision/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_session_type" type="string"/>
      <bind nodeset="/covid19-rdt-provision/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_provision_mode" type="string"/>
      <bind nodeset="/covid19-rdt-provision/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_provision_mode_data" type="string"/>
      <bind calculate="../../../../../inputs/contact/_id" nodeset="/covid19-rdt-provision/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_flavor_one" type="string"/>
      <bind calculate="../../../../../inputs/contact/name" nodeset="/covid19-rdt-provision/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_flavor_two" type="string"/>
      <bind nodeset="/covid19-rdt-provision/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_cloudworks_dns" type="string"/>
      <bind nodeset="/covid19-rdt-provision/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_state" required="true()" type="string"/>
      <bind nodeset="/covid19-rdt-provision/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_time_started" type="string"/>
      <bind nodeset="/covid19-rdt-provision/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_time_resolved" type="string"/>
      <bind nodeset="/covid19-rdt-provision/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_time_expired" type="string"/>
      <bind nodeset="/covid19-rdt-provision/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_test_profile" type="string"/>
